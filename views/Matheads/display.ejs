<head><title>Consultar Material - MATHEADS</title><script src="/bower_components/webcomponentsjs/webcomponents.min.js"></script>	<link rel="stylesheet" href="/styles/app.css"><link rel="import" href="/bower_components/paper-material/paper-material.html"><link rel="import" href="/bower_components/paper-button/paper-button.html"></head><paper-material class="card" elevation="3" vertical layout><h2 class="card_title">Consultar Material - MATHEADS</h2><form id="form"><div vertical layout><div class="card_line"><label class="left_label" for="matnr">NÃºmero Material</label><input id="matnr" type="number" name="matnr" value="<%= matnr%>" disabled></div><div class="card_line"><label class="left_label" for="matl_type">Tipo de Material</label><select id="matl_type" name="matl_type" disabled><option value="ZMED">ZMED - MEDICINAS</option><option value="ZMIS">ZMIS - MISCELANEOS</option></select></div><div class="card_line"><label class="left_label" for="matl_group">Grupo de Material</label><input id="matl_group" type="text" name="matl_group" value="<%= matl_group%>" disabled></div><div class="card_line"><label class="left_label" for="matl_cat">Categoria de Material</label><input id="matl_cat" type="text" name="matl_cat" value="<%= matl_cat%>" disabled></div><input id="create_btn" type="submit" value="SALIR"><div class="card_line"><paper-button id="btn1" class="btn" raised>SALIR</paper-button></div></div></form></paper-material><script>document.getElementById("btn1").addEventListener("click", function() {document.getElementById("create_btn").click();});</script><script>document.getElementById("matl_type").value = "<%= matl_type%>";</script>